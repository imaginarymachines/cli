#!/usr/bin/env node

switch(process.argv[2]){
    case 'plugin':
        require = require('esm')(module /*, options*/);
        require('../src/plugin.js').cli(process.argv);
    break;
    case 'login':
        require = require('esm')(module /*, options*/);
        require('../src/login.js').cli(process.argv);
    break;
    case '-v':
    case 'v':
    case 'version':
        require = require('esm')(module /*, options*/);
        require('../src/version.js').cli(process.argv);
        break;
    default:
        require = require('esm')(module /*, options*/);
        require('../src/debug.js').cli(process.argv);
        break;
    break;

}
